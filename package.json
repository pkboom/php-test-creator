{
    "name": "php-test-creator",
    "displayName": "PHP Test Creator",
    "description": "Create Test",
    "version": "0.0.1",
    "publisher": "pkboom",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:create.test"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "create.test",
                "title": "Create Test"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "PHP Test Creator configurations",
            "properties": {
                "phpTestCreator.withoutExceptionHandling.visiblity": {
                    "type": "boolean",
                    "default": "false",
                    "description": "Show/Hide withoutExceptionHandling"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pkboom/php-test-creator.git"
    }
}
